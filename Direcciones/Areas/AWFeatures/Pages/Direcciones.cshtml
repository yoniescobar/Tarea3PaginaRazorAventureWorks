@page
@using AdventureWorksNS.Data
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model AWFeatures.Pages.DireccionesModel


<div class="row">
    <h1 class="display-2">Direcciones</h1>
    <div class="row">
        @foreach (Address address in Model.Direcciones)
        {
            <div class="col-sm-3">
                <partial name="_Direccion" model="address" />
            </div>
        }
    </div>
</div>